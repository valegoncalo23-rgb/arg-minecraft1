def gerar_codigo(partes, indices):
    return "".join([partes[i] for i in indices])

partes = ["x", "&t=", "y", "1", "s", "z"]
indices = [1, 3, 4]  # Ordem que precisamos para o código final

codigo = gerar_codigo(partes, indices)

if codigo == "&t=1s":
    print("Código correto! Acesso concedido:", codigo)
else:
    print("Código incorreto. Tente novamente.")
